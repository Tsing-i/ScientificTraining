            <ColumnDefinition Width="433"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Width="393">
            <Grid.RowDefinitions>
                <RowDefinition Height="494"/>
                <RowDefinition Height="435"/>
            </Grid.RowDefinitions>
            <ContentControl Margin="0,10,0,0" prism:RegionManager.RegionName="TimeAreaContentRegion1"/>
            <ContentControl Grid.Row="1" prism:RegionManager.RegionName="AthletesShowContentRegion"/>
