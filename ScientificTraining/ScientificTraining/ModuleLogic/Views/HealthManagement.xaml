                                                        <Label Content="最近体测"/>
                                                        <Label Content="{Binding physical_age_lately}" FontWeight="Bold" Foreground="#C3446B"/>
                                                        <Border Height="1" Background="#D9E1F3"/>
                                                        <Label Content="变化趋势"/>
                                                        <Label Margin="0,-5,0,0" Content="{Binding  physical_age_trend}" Foreground="#0BA1A7" FontWeight="Bold"/>
                                                    </StackPanel>
                                                </Border>
                                            </Grid>
                                        </Border>
                                    </Grid>
