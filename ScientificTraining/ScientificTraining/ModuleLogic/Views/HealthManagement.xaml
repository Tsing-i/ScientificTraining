        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/HealthManagementStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
